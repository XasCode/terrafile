{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "pretty": true,
    "sourceMap": true,
    "target": "es5",
    "resolveJsonModule": true,
    "outDir": "./dist",
    "noImplicitAny": true,
    "esModuleInterop": true
},
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}