    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-progress-bar-reporter": "^1.0.21",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "prettier-eslint": "^12.0.0",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
