  },
  "homepage": "https://github.com/XasCode/terrafile#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "chalk": "^4.1.1",
    "commander": "^8.1.0",
    "fast-glob": "^3.2.7",
    "fs-extra": "^9.1.0",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "touch": "^3.1.0"
  },
  "devDependencies": {
    "@jestaubach/eslint-config": "^0.0.1",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/mkdirp": "^1.0.1",
